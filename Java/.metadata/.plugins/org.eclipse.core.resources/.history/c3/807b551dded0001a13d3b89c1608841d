
public class Student extends Person{
	private int mark;
	
	Student(String nameStudent,int ageStudent)
	{
		this.name = nameStudent;
		this.age = ageStudent;
	}
	public void setMark(int mark)
	{
		this.mark = mark;
	}
	
	public int getMark()
	{
		return mark;
	}
	
	public String toString()
	{
		return name + ","+age;
	}
}
