
public class Student {
	private String name;
	private int age;
	private int mark;
	
	Student(String nameStudent,int ageStudent)
	{
		this.name = nameStudent;
		this.age = ageStudent;
	}
	public void setMark(int mark)
	{
		this.mark = mark;
	}
	
	public String toString()
	{
		return name + ","+age;
	}
}
