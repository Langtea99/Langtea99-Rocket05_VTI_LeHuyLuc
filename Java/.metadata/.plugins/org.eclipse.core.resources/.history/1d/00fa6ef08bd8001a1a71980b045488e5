import java.util.Scanner;

public class Person {
	private int age;

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	public int inputAge(){
		try{
			Scanner scanner = new Scanner(System.in);
			System.out.println("Nhap vao tuoi : ");
			int age = scanner.nextInt();
			
		}catch{
			
		}
		return age;
	}
}
