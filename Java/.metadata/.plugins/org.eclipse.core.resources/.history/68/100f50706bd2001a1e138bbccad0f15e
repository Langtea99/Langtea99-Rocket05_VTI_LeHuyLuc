package Inheritance;

import java.util.Scanner;

public class CongNhan extends CanBo{
	private byte bac;

	public CongNhan()
	{
		
	}
	public byte getBac() {
		return bac;
	}

	public CongNhan(byte bac) {
		super();
		this.bac = bac;
	}
	public void setBac(byte bac) {
		this.bac = bac;
	}
	public String toString()
	{
		return super.toString()+"bac"+bac;
	}
}
