package Inheritance;

public class CanBo {
	private String hoTen;
	private byte tuoi;
	private GioiTinh gioiTinh;
	private String diaChi;
	
	public CanBo() 
	{
	}

	public CanBo(String hoTen, byte tuoi, GioiTinh gioiTinh, String diaChi) {
		super();
		this.hoTen = hoTen;
		this.tuoi = tuoi;
		this.gioiTinh = gioiTinh;
		this.diaChi = diaChi;
	}

	public String getHoTen() {
		return hoTen;
	}

	public void setHoTen(String hoTen) {
		this.hoTen = hoTen;
	}

	public byte getTuoi() {
		return tuoi;
	}

	public void setTuoi(byte tuoi) {
		this.tuoi = tuoi;
	}

	public GioiTinh getGioiTinh() {
		return gioiTinh;
	}

	public void setGioiTinh(int gioiTinh) {
		if (gioiTinh == 0) {
			this.gioiTinh = GioiTinh.valueOf("MALE");
		} else if (gioiTinh == 1) {
			this.gioiTinh = GioiTinh.valueOf("FEMALE");
		} else {
			this.gioiTinh = GioiTinh.valueOf("UNKNOWN");
		}
	}

	public String getDiaChi() {
		return diaChi;
	}

	public void setDiaChi(String diaChi) {
		this.diaChi = diaChi;
	}

}
