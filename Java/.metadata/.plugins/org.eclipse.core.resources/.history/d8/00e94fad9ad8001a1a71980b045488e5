import java.util.Scanner;

public class Question5 {

	public static void main(String[] args) {
		Person person = new Person();
		Scanner scanner = new Scanner(System.in);
			try {
				System.out.print("Nhap vao tuoi :");
				int age = scanner.nextInt();
				person.checkAge(age);
			} catch (Exception e) {
				e.printStackTrace();
			}
	}
}
